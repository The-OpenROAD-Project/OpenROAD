VERSION 5.8 ; 
DIVIDERCHAR "/" ;
BUSBITCHARS "[]" ;

DESIGN buffer_ports6 ;

UNITS DISTANCE MICRONS 1000 ;

DIEAREA ( 0 0 ) ( 2000 2000 ) ;

COMPONENTS 1 ;
- r1 DFFR_X1 + PLACED ( 5000 0 ) N ;
END COMPONENTS

PINS 2 ;
- RESET + NET RESET + DIRECTION INPUT + USE SIGNAL + SPECIAL ;
- R1_OUT + NET R1_OUT + DIRECTION OUTPUT + USE SIGNAL + SPECIAL ;
END PINS

SPECIALNETS 4 ;
- VSS  ( * VSS )
  + USE GROUND ;
- VDD  ( * VDD )
  + USE POWER ;
- RESET ( PIN RESET ) ( * RN ) 
  + USE SIGNAL ;
- R1_OUT ( PIN R1_OUT ) ( r1 Q ) 
  + USE SIGNAL ;
END SPECIALNETS

END DESIGN
