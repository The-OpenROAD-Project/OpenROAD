VERSION 5.8 ; 
NAMESCASESENSITIVE ON ;
DIVIDERCHAR "/" ;
BUSBITCHARS "[]" ;

DESIGN top ;

UNITS DISTANCE MICRONS 1000 ;

DIEAREA ( 0 0 ) ( 1000 1000 ) ;


COMPONENTS 1 ;
- u1 BUF_X1 ;
END COMPONENTS

PINS 4 ;
- in1 + NET in1 + DIRECTION INPUT ;
- out1 + NET out1 + DIRECTION OUTPUT ;
- VDD + NET VDD + DIRECTION INPUT + USE POWER ;
- VSS + NET VSS + DIRECTION INPUT + USE GROUND ;
END PINS

SPECIALNETS 2 ;
- VSS  ( * VSS )
  + USE GROUND ;
- VDD  ( * VDD )
  + USE POWER ;
END SPECIALNETS

NETS 2 ;
- in1 ( PIN in1 ) ( u1 A ) ;
- out1 ( u1 Z ) ( PIN out1 ) ;
END NETS

END DESIGN
