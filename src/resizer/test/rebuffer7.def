###############################################################################
# r1 driving r2-r5 from end with pin at end
###############################################################################

VERSION 5.5 ; 
NAMESCASESENSITIVE ON ;
DIVIDERCHAR "/" ;
BUSBITCHARS "[]" ;

DESIGN reg1 ;

UNITS DISTANCE MICRONS 1000 ;

DIEAREA ( 0 0 ) ( 1000 1000 ) ;

COMPONENTS 5 ;
- r1 snl_ffqx1 + PLACED   ( 100000 100000 ) N ;
- r2 snl_ffqx1 + PLACED   ( 100000 200000 ) N ;
END COMPONENTS

PINS 1 ;
- clk + NET clk + DIRECTION INPUT + USE CLOCK 
  + LAYER M1 ( 0 0 ) ( 0 100 ) + FIXED ( 100000 100000 ) N ;
- out + NET out + DIRECTION OUTPUT + USE SIGNAL
  + LAYER M1 ( 0 0 ) ( 0 100 ) + FIXED ( 100000 600000 ) N ;
END PINS

SPECIALNETS 2 ;
- VSS  ( * VSS )
  + USE GROUND ;
- VDD  ( * VDD )
  + USE POWER ;
END SPECIALNETS

NETS 2 ;
- clk ( PIN clk ) ( r1 CP ) ( r2 CP ) ;
- out ( r1 Q ) ( r2 D ) ( PIN out ) ;
END NETS

END DESIGN
