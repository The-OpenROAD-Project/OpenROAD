###############################################################################
# reg1.def with hier inst, net names (escaped and unescaped)
###############################################################################

VERSION 5.5 ; 
NAMESCASESENSITIVE ON ;
DIVIDERCHAR "/" ;
BUSBITCHARS "[]" ;

DESIGN reg1 ;

UNITS DISTANCE MICRONS 1000 ;

DIEAREA ( -1000 -1000 ) ( 1000 1000 ) ;


COMPONENTS 5 ;
- h1/r1 snl_ffqx1 ;
- r2 snl_ffqx1 ;
- r3 snl_ffqx1 ;
- h2\/u1 snl_bufx1 ;
- u2 snl_and02x1 ;
END COMPONENTS

PINS 6 ;
- in1 + NET in1 + DIRECTION INPUT ;
- in2 + NET in2 + DIRECTION INPUT ;
- clk1 + NET clk1 + DIRECTION INPUT ;
- clk2 + NET clk2 + DIRECTION INPUT ;
- clk3 + NET clk3 + DIRECTION INPUT ;
- out + NET out + DIRECTION OUTPUT ;
END PINS

SPECIALNETS 2 ;
- VSS  ( * VSS )
  + USE GROUND ;
- VDD  ( * VDD )
  + USE POWER ;
END SPECIALNETS

NETS 10 ;
- in1 ( PIN in1 ) ( h1/r1 D ) ;
- in2 ( PIN in2 ) ( r2 D ) ;
- clk1 ( PIN clk1 ) ( h1/r1 CP ) ;
- clk2 ( PIN clk2 ) ( r2 CP ) ;
- clk3 ( PIN clk3 ) ( r3 CP ) ;
- h1/r1q ( h1/r1 Q ) ( u2 A ) ;
- r2q ( r2 Q ) ( h2\/u1 A ) ;
- h2\/u1z ( h2\/u1 Z ) ( u2 B ) ;
- u2z ( u2 Z ) ( r3 D ) ;
- out ( r3 Q ) ( PIN out ) ;
END NETS

END DESIGN
