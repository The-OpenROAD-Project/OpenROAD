VERSION 5.8 ;
DIVIDERCHAR "/" ;
BUSBITCHARS "[]" ;
DESIGN top ;
UNITS DISTANCE MICRONS 1000 ;
DIEAREA ( 0 0 ) ( 1000 1000 ) ;
COMPONENTS 2 ;
    - b1 BUF_X1 ;
    - b4 BUF_X8 ;
END COMPONENTS
PINS 2 ;
    - in1 + NET in1 + DIRECTION INPUT + USE SIGNAL ;
    - out1 + NET out1 + DIRECTION OUTPUT + USE SIGNAL ;
END PINS
SPECIALNETS 2 ;
    - VSS ( * VSS ) + USE GROUND ;
    - VDD ( * VDD ) + USE POWER ;
END SPECIALNETS
NETS 3 ;
    - in1 ( PIN in1 ) ( b1 A ) + USE SIGNAL ;
    - out1 ( PIN out1 ) ( b4 Z ) + USE SIGNAL ;
    - n1 ( b4 A ) ( b1 Z ) + USE SIGNAL ;
END NETS
END DESIGN
