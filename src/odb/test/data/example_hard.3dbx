Header:
  version: 3
  unit: micron
  precision: 2000
  include:
    - hard.3dbv
Design:
  name: TopDesign
ChipletInst:
  soc_inst:
    reference: SoC
  soc_inst_duplicate:
    reference: SoC
Stack:
  soc_inst:
    loc: [0, 0]
    z: 0.0
    orient: R0
  soc_inst_duplicate:
    loc: [0, 0]
    z: 300.01
    orient: R0
Connection:
  soc_to_soc:
    top: soc_inst_duplicate.regions.back_reg
    bot: soc_inst.regions.front_reg
    thickness: 50.0
