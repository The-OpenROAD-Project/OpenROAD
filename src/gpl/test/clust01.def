VERSION 5.8 ;
DIVIDERCHAR "/" ;
BUSBITCHARS "[]" ;
DESIGN tray_test ;
UNITS DISTANCE MICRONS 1000 ;
DIEAREA ( 0 0 ) ( 1000 1000 ) ;


COMPONENTS 4 ;
- ff1 FF + PLACED ( 6000 6000 ) N ;
- ff2 FF + PLACED ( 4000 6000 ) N ;
- ff3 FF + PLACED ( 4000 4000 ) N ;
- ff4 FF + PLACED ( 6000 4000 ) N ;
END COMPONENTS

PINS 1 ;
- clk1 + NET clk1 + DIRECTION INPUT + USE SIGNAL + FIXED ( 1000 1000 ) N + LAYER M1 ( 0 0 ) ( 0 0 ) ;
END PINS


NETS 1 ;
- clk1 ( PIN clk1 ) ( ff1 CLK ) ( ff2 CLK ) ( ff3 CLK ) ( ff4 CLK ) + USE SIGNAL ;
END NETS

END DESIGN