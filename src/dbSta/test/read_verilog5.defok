VERSION 5.8 ;
NAMESCASESENSITIVE ON ;
DIVIDERCHAR "/" ;
BUSBITCHARS "[]" ;
DESIGN top ;
UNITS DISTANCE MICRONS 1000 ;
COMPONENTS 1 ;
    - b1 bidir ;
END COMPONENTS
PINS 3 ;
    - in1 + NET in1 + DIRECTION INPUT + USE SIGNAL ;
    - in2 + NET in2 + DIRECTION INPUT + USE SIGNAL ;
    - out1 + NET out1 + DIRECTION OUTPUT + USE SIGNAL ;
END PINS
NETS 3 ;
    - in1 ( PIN in1 ) ( b1 IO ) + USE SIGNAL ;
    - in2 ( PIN in2 ) ( b1 A ) + USE SIGNAL ;
    - out1 ( PIN out1 ) ( b1 Z ) + USE SIGNAL ;
END NETS
END DESIGN
