VERSION 5.8 ;
DIVIDERCHAR "/" ;
BUSBITCHARS "[]" ;
DESIGN top ;
UNITS DISTANCE MICRONS 1000 ;
DIEAREA ( 0 0 ) ( 1000 1000 ) ;
COMPONENTS 1 ;
    - i1 INV_X1 ;
END COMPONENTS
PINS 3 ;
    - in1 + NET in1 + DIRECTION INPUT + USE SIGNAL ;
    - out1 + NET out1 + DIRECTION OUTPUT + USE SIGNAL ;
    - out2 + NET out1 + DIRECTION OUTPUT + USE SIGNAL ;
END PINS
SPECIALNETS 2 ;
    - VDD ( * VDD ) + USE POWER ;
    - VSS ( * VSS ) + USE GROUND ;
END SPECIALNETS
NETS 2 ;
    - in1 ( PIN in1 ) ( i1 A ) + USE SIGNAL ;
    - out1 ( PIN out2 ) ( PIN out1 ) ( i1 ZN ) + USE SIGNAL ;
END NETS
END DESIGN
